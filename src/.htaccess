RewriteEngine On
RewriteBase /

#---- Commented for local start -----#

#RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]

#RewriteCond %{REQUEST_URI} !^/[A-F0-9]{32}\.txt(?:\ Comodo\ DCV)?$
